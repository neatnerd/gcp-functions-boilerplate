{
  "name": "gcp-functions-boilerplate",
  "version": "0.0.1",
  "description": "Template for GCP Cloud Functions",
  "main": "index.js",
  "localPort": 8080,
  "scripts": {
    "background-function": "functions-framework --target=background_function --signature-type=event --port $npm_package_localPort",
    "http-function": "functions-framework --target=http_function --signature-type=http --port $npm_package_localPort"
  },
  "keywords": [
    "gcp",
    "cloud-functions"
  ],
  "author": "Arshinskiy Mike",
  "license": "ISC",
  "devDependencies": {
    "@google-cloud/functions-framework": "^1.5.1",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0"
  }
}
